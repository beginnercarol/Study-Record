{
	"name" : "GetHTTP",
	"version" : "1.0",
	"manifest_version" : 2,
	"icons":{"16":"icon.png","128":"icon.png"},
	"browser_action":{
		"default_title" : "",
		"default_icon":"icon.png"
	},
	"background":{
		"scripts" : ["background.js"],
		"persistent" : false
	},
	"devtools_page" : "devtools.html",
	"permissions": [
	    "http://*/*",
	    "https://*/*"
  	]
}